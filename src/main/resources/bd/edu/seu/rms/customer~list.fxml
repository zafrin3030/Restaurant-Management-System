            <ImageView fitHeight="32.0" fitWidth="38.0" layoutX="14.0" layoutY="20.0" onMouseClicked="#goToAdminDashboard" pickOnBounds="true">
               <image>
                  <Image url="@../../../../Binty_Project_Image/e.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <TableView fx:id="customerListTable" layoutX="26.0" layoutY="95.0" prefHeight="475.0" prefWidth="695.0" style="-fx-border-color: #000000; -fx-background-color: #dcdcdc;">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="49.0" style="-fx-border-color: #000000; -fx-background-color: #dcdcdc;" text="ID" />
          <TableColumn fx:id="fullNameCol" prefWidth="149.0" style="-fx-border-color: #000000; -fx-background-color: #dcdcdc;" text="Full_name" />
