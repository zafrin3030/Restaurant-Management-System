            <TableColumn fx:id="addressCol" prefWidth="194.0" style="-fx-border-color: #000000; -fx-background-color: #dcdcdc;" text="Address" />
            <TableColumn fx:id="genderCol" prefWidth="74.0" style="-fx-border-color: #000000; -fx-background-color: #dcdcdc;" text="Gender" />
        </columns>
      </TableView>
      <TextField fx:id="searchField" layoutX="136.0" layoutY="46.0" onKeyPressed="#searchKeyValue" prefHeight="25.0" prefWidth="441.0" promptText="Search here" style="-fx-border-radius: 60; -fx-border-color: #000000; -fx-background-radius: 60;" />
   </children>
</AnchorPane>
