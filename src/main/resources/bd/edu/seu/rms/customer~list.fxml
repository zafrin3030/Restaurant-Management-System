<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bd.edu.seu.rms.controllers.CustomerListController">
   <children>
      <AnchorPane layoutX="739.0" prefHeight="600.0" prefWidth="61.0" style="-fx-background-color: #4a4a7a;">
